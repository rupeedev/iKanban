{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                tt.id AS \"id!: Uuid\",\n                tt.tag_id AS \"tag_id!: Uuid\",\n                t.tag_name AS \"tag_name!\",\n                t.content AS \"content!\",\n                t.color,\n                tt.created_at AS \"created_at!: DateTime<Utc>\"\n            FROM task_tags tt\n            JOIN tags t ON t.id = tt.tag_id\n            WHERE tt.task_id = $1\n            ORDER BY t.tag_name ASC\n            ",
