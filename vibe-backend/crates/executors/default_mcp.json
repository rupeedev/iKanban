    ]
  },
  "cloudflare": {
    "command": "npx",
    "args": [
      "-y",
      "@cloudflare/mcp-server"
    ],
    "env": {
      "CLOUDFLARE_API_TOKEN": "YOUR_CF_TOKEN"
    }
  },
